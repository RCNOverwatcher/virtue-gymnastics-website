{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"files": {
		"ignore": [
			".next",
			"dist",
			"node_modules",
			"out",
			"public",
			"vercel",
			"vercel.json",
			"yarn.lock"
		]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
